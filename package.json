{
  "name": "react-19-sandbox",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "biome lint ./src",
    "fix": "biome lint --write ./src",
    "fmt": "biome format --write ./src",
    "coverage": "vitest run --coverage",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@testing-library/jest-dom": "6.5.0",
    "@testing-library/react": "16.0.1",
    "@testing-library/user-event": "14.5.2",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "4.3.1",
    "globals": "15.9.0",
    "jsdom": "25.0.0",
    "typescript": "5.5.4",
    "vite": "5.4.2",
    "vitest": "2.0.5"
  }
}
